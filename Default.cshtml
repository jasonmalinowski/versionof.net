@{
    Layout = null;

    var document = System.Xml.Linq.XDocument.Load(Server.MapPath("~/VersionData.xml"));
}

<html>
<head>
    <title>versionof.net</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <style>
        /* Icon when the collapsible content is shown */
        .expandableButton:after {
            font-family: "Glyphicons Halflings";
            content: "\e114";
            float: right;
            margin-left: 5px;
        }
        /* Icon when the collapsible content is hidden */
        .expandableButton.collapsed:after {
            content: "\e080";
        }

        .btn-info {
            width: 120px;
            text-align: left;
            padding-right: 5px;
            padding-top: 0px;
            padding-bottom: 0px;
            padding-left: 5px;
            margin-right: 0px;
            margin-top: 1px;
            margin-bottom: 0px;
            margin-left: 0px;
            background-color: #68217A;
            border-color: #68217A;
        }

            .btn-info:hover {
                background-color: #68217A;
                border-color: #68217A;
            }

            .btn-info:focus {
                background-color: #68217A;
                border-color: #68217A;
            }

            .btn-info:selected {
                background-color: #68217A;
                border-color: #68217A;
            }

        .details {
            padding-left: 25px;
        }

        .date {
            font-weight: bold;
            display: inline-block;
            width: 85px;
            margin-left: 5px;
        }

        .version {
            font-style: italic;
            display: inline-block;
            width: 150px;
        }
    </style>
</head>

<body>
    @foreach (var majorRelease in document.Element("versionData").Elements("majorRelease"))
    {
        <h2>@majorRelease.Attribute("name").Value</h2>
        <ul>
            @foreach (var minorRelease in majorRelease.Elements("minorRelease"))
            {
                var collapsableTagId = (majorRelease.Attribute("name").Value + minorRelease.Attribute("name").Value).Replace(" ", "").Replace(".", "").ToLower();
                <li>
                    <button type="button" class="expandableButton btn-info collapsed" data-toggle="collapse" data-target="#@collapsableTagId">@minorRelease.Attribute("name").Value</button>
                    <span class="date">(@minorRelease.Attribute("date").Value)</span> - <span class="version">@minorRelease.Attribute("aboutHelpVersion").Value</span>
                    <div id="@collapsableTagId" class="details collapse">
                        @{ var roslynInfo = minorRelease.Element("roslyn"); }

                        @if (roslynInfo != null)
                        {
                            var roslynTag = roslynInfo.Element("git").Attribute("tag").Value;
                            var roslynSha = roslynInfo.Element("git").Attribute("sha1").Value;

                            <div>
                                <b>Roslyn Tag:</b> <a href="https://github.com/dotnet/roslyn/releases/tag/@roslynTag">@roslynTag</a><br />
                                <b>Roslyn SHA:</b> <a href="https://github.com/dotnet/roslyn/commit/@roslynSha">@roslynSha</a><br />
                                <b>Roslyn Build:</b> @roslynInfo.Element("build").Attribute("fullNumber").Value from @roslynInfo.Element("build").Attribute("branch").Value
                            </div>
                        }
                    </div>
                </li>
            }
        </ul>
    }

